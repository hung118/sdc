	<queryString><![CDATA[select 
	upper(p.firstname) + ' ' + upper(p.lastname) as testername,
	p.languages as languages,
	upper(s.name) as schoolname,
	upper(s.address1) as address1,
	upper(s.address2) as address2,
	upper(s.city) as city,
	s.businessphone,
	s.state as state,
	s.zip as zip
from person_school ps, person p, school s
where 
	ps.person_fk = p.person_pk and
	ps.person_fk in (select pr.person_fk from person_roles pr where pr.role = 'THIRDPARTY') and
	ps.school_fk = s.school_pk
order by testername, schoolname
]]></queryString>

