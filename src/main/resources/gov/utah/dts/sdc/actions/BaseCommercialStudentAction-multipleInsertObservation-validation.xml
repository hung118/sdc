<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE validators PUBLIC "-//Apache Struts//XWork Validator 1.0.3//EN" "http://struts.apache.org/dtds/xwork-validator-1.0.3.dtd">
<validators>
   <!-- Field-Validator Syntax -->
    <field name="observationDate">
         <field-validator type="required">
            <message key="commercial.observationDate.required"/>
        </field-validator>
    </field>
 
    <field name="vehicleFk">
         <field-validator type="required">
            <message key="commercial.vehicleFk.required"/>
        </field-validator>
    </field>
    
    <field name="instructorFk">
         <field-validator type="required">
            <message key="commercial.instructorFk.required"/>
        </field-validator>
    </field>
    
       <field name="observationStartTime">
        <field-validator type="requiredstring">
            <param name="trim">true</param>
            <message key="commercial.field.required"/>
        </field-validator>
    </field>
    
    <field name="observationEndTime">
        <field-validator type="requiredstring">
            <param name="trim">true</param>
            <message key="commercial.field.required"/>
        </field-validator>
    </field>
   
  
   
    
  </validators>
